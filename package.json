{
  "name": "elastalert-kibana-plugin",
  "version": "1.0.0-alpha.1",
  "description": "Create, test and edit ElastAlert rules within Kibana.",
  "main": "index.js",
  "license": "SEE LICENSE IN LICENSE.md",
  "kibana": {
    "version": "6.3.3",
    "templateVersion": "1.0.0"
  },
  "scripts": {
